% Программа находит n-ое число Фибоначчи

def Fib (var num)
{
    if (num <= 1)
    {
        return (num)
    }

    return (Fib (num - 1) + Fib (num - 2))
}

def main ()
{
    var m = 1
    var n = 20

    var i = 0
	
    while (i < n + 1)
    {
        print (Fib (i))	    

	i = i + 1
    }
	
    return (0)
}