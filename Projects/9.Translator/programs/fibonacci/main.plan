
$$ Программа находит n-ое число Фибоначчи

def Fib (var num)
{
    if (num <= 1)
    {
        return (num)
    }

    return (Fib (num - 1) + Fib (num - 2))
}

def main ()
{
    putc ('M')
    putc (':')
    var m = input ()
    var n = input ()

    if (m == 1)
    {
        var i = 0

        while (i <= n)
        {
            print (Fib (i)) putc(' ')
            i = i + 1
        }
    }
    else
    {
        print (Fib (n))
    }

    return (0)
}